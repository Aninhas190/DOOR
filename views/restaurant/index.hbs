<h1>Restaurant index page</h1>
<div class="restaurant-index">
  <a href="/restaurant/createByZomatoId">Pre existing Zomato Restaurant</a>
  
  <a href="/restaurant/create">Create new Restaurant</a>
</div>

<ul>
  {{#each restaurants}}
  <li>
    <h3>
      <a href="/restaurant/{{_id}}">{{name}}</a>
    </h3>
    <p><strong>Cuisine Type: </strong>{{cuisineType}}</p>
    <p><strong>Restaurant Description:</strong>{{description}}</p>
    </br>
  </li>
  {{/each}}
</ul>
